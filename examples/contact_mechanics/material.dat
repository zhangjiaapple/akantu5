material elastic [
         name = upper
         rho = 1.0   # density
         E   = 1e3 # young's modulu
         nu  = 0.3    # poisson's ratio
]

material elastic [
         name = lower
         rho = 1.0   # density
         E   = 1e3 # young's modulu
         nu  = 0.3   # poisson's ratio
]

contact_detector [
  type = explicit		 			
  master = contact_bottom
  slave = contact_top
  projection_tolerance = 1e-10
  max_iterations = 100
  extension_tolerance = 1e-5
]

contact_resolution penalty_linear [
  name = contact_top
  mu = 0.0
  epsilon_n = 4e5
  epsilon_t = 1e5
  is_master_deformable = false
]
