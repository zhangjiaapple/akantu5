material elastic [
         name = upper
         rho = 1230   # density
         E   = 7e7   # young's modulu
         nu  = 0.0   # poisson's ratio
]

material elastic [
         name = lower
         rho = 1230   # density
         E   = 7e7   # young's modulu
         nu  = 0.0   # poisson's ratio
]

contact_detector [
  type = explicit
  max_iterations = 1000
  projection_tolerance = 1e-10
  extension_tolerance = 1e-3		 			
  master = contact
  slave = contact
]

contact_resolution penalty_linear [
  name = contact
  mu = 0.0
  epsilon_n = 1050e7
  epsilon_t = 0
  is_master_deformable = true
]