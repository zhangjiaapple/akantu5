material elastic [
         name = upper
         rho = 1230  # density
         E   = 7e7 # young's modulu
         nu  = 0.0    # poisson's ratio
]

material elastic [
         name = top_body
         rho = 1230   # density
         E   = 7e7 # young's modulu
         nu  = 0.0   # poisson's ratio
]

contact_detector [
  type = explicit		 			
  master = contact
  slave = contact
  projection_tolerance = 1e-10
  max_iterations = 100
  extension_tolerance = 1e-5
]

contact_resolution penalty_linear [
  name = contact
  mu = 0.0
  epsilon_n = 1050e7
  epsilon_t = 0.0
  is_master_deformable = false
]