Yd0 = 94e3

material marigo_non_local [
         name = concrete
         rho = 3900    # density
         E   = 370e9    # young's modulus
         nu  = 0.22     # poisson's ratio
         Yd  = Yd0 uniform [ 0, 0.1 * Yd0 ]
         Sd  = 2.748e6
]

non_local concrete remove_wf [
	  radius       = 0.01
	  weight_function weight_parameter [
	  		  damage_limit = 0.99
			  	  		  update_rate  = 10	       
	  ]	  
]
